{
    "name": "Hide youtube videos",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["content.js"],
            "css": ["content.css"]
        }
    ],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
      },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}